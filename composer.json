{
    "name": "daseraf/magento2-debug-creatuity",
    "description": "Compatibility module for creatuity/magento2-interceptors",
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "magento/framework": "~101.0|~102.0|~103.0",
        "magento/module-backend": "~100.2|~101.0|~102.0",
        "magento/module-developer": "^100.2",
        "daseraf/magento2-debug": "^2.2.1",
        "creatuity/magento2-interceptors": "^1.3",
        "daseraf/magento2-debug-theme": "^1.0",
        "filp/whoops": "^2.1",
        "jdorn/sql-formatter": "^1.2",
        "symfony/var-dumper": "*",
        "symfony/stopwatch": "^2.8 || ^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "config": {
        "preferred-install": "source"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "dbg-theme": {
            "type": "git",
            "url": "https://github.com/daseraf/magento2-debug-theme.git"
        }
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Daseraf\\DebugCreatuity\\": ""
        }
    }
}
